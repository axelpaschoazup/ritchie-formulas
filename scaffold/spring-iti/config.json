{
  "description": "Create your microservice scaffold",
  "inputs" : [
    {
      "name" : "packaging",
      "type" : "text",
      "default" : "jar",
      "label" : "Pick your packaging type: ",
      "items": ["jar"]
    },
    {
      "name" : "java_version",
      "type" : "text",
      "default" : "11",
      "label" : "Pick your Java version: ",
      "items": ["11"]
    },
    {
      "name" : "language",
      "type" : "text",
      "default" : "kotlin",
      "items" : ["kotlin", "java"],
      "label" : "Select your language: "
    },
    {
      "name" : "group_id",
      "type" : "text",
      "label" : "Type your groupId[ex.: itau.iti.demo]: "
    },
    {
      "name" : "artifact_id",
      "type" : "text",
      "label" : "Type your artifactId[ex.: demo]: "
    },
    {
      "name" : "version",
      "type" : "text",
      "default" : "0.0.1-SNAPSHOT",
