    },
    {
      "name" : "name",
      "type" : "text",
      "label" : "Type your project name[ex.: demo]: "
    },
    {
      "name" : "description",
      "type" : "text",
      "label" : "Type the project`s description [ex.: project demo]: "
    },
    {
      "name" : "package_name",
      "type" : "text",
      "label" : "Type your package name[ex.: itau.iti.demo]: "
    },
    {
      "name" : "dependencies",
      "type" : "text",
      "label" : "Select one or more dependencies [iti-web,iti-dynamodb,iti-kafka]: "
    }
  ]
}